var items_container = $('div.features_items');
$(items_container).html('');

<% @items.each_with_index do |item, index| %>
    $(items_container).append($('<%=j render partial: 'items/item', locals: {item: item, sub_category: @sub_category} %>'));
<% end %>

$('div.pagination_container').html($('<%=j paginate @items %>'));
